{
  "$defs": {
    "AccessRoles": {
      "description": "Defines access roles such that a larger value always corresponds to more\npermissions. `0` equates to no permission, negative numbers correspond to\nnon-user roles. In this way, an activity can require, for example,\n`permission > AccessRoles.GUEST` to grant access to all registered users,\nadmins, and owners.",
      "enum": [
        0,
        1,
        2,
        3,
        4,
        -1
      ],
      "title": "AccessRoles",
      "type": "integer"
    },
    "BrainForgeConfig": {
      "description": "Defines configuration used in BrainForge LLM applications.",
      "properties": {
        "inference_access": {
          "additionalProperties": {
            "additionalProperties": {
              "items": {
                "type": "string"
              },
              "type": "array"
            },
            "type": "object"
          },
          "default": {},
          "title": "Inference Access",
          "type": "object"
        }
      },
      "title": "BrainForgeConfig",
      "type": "object"
    },
    "KlatConfig": {
      "description": "Defines user configuration used in PyKlatChat.",
      "properties": {
        "is_tmp": {
          "default": true,
          "title": "Is Tmp",
          "type": "boolean"
        },
        "preferences": {
          "default": {},
          "title": "Preferences",
          "type": "object"
        }
      },
      "title": "KlatConfig",
      "type": "object"
    },
    "NeonUserConfig": {
      "description": "Defines user configuration used in Neon Core.",
      "properties": {
        "skills": {
          "additionalProperties": {
            "type": "object"
          },
          "default": {},
          "title": "Skills",
          "type": "object"
        },
        "user": {
          "allOf": [
            {
              "$ref": "#/$defs/_UserConfig"
            }
          ],
          "default": {
            "first_name": "",
            "middle_name": "",
            "last_name": "",
            "preferred_name": "",
            "dob": null,
            "email": "",
            "avatar_url": "",
            "about": "",
            "phone": "",
            "phone_verified": false,
            "email_verified": false
          }
        },
        "language": {
          "allOf": [
            {
              "$ref": "#/$defs/_LanguageConfig"
            }
          ],
          "default": {
            "input_languages": [
              "en-us"
            ],
            "output_languages": [
              "en-us"
            ]
          }
        },
        "units": {
          "allOf": [
            {
              "$ref": "#/$defs/_UnitsConfig"
            }
          ],
          "default": {
            "time": 12,
            "date": "MDY",
            "measure": "imperial"
          }
        },
        "location": {
          "allOf": [
            {
              "$ref": "#/$defs/_LocationConfig"
            }
          ],
          "default": {
            "latitude": null,
            "longitude": null,
            "name": null,
            "timezone": null
          }
        },
        "response_mode": {
          "allOf": [
            {
              "$ref": "#/$defs/_ResponseConfig"
            }
          ],
          "default": {
            "hesitation": false,
            "limit_dialog": false,
            "tts_gender": "female",
            "tts_speed_multiplier": 1.0
          }
        },
        "privacy": {
          "allOf": [
            {
              "$ref": "#/$defs/_PrivacyConfig"
            }
          ],
          "default": {
            "save_text": true,
            "save_audio": false
          }
        }
      },
      "title": "NeonUserConfig",
      "type": "object"
    },
    "PermissionsConfig": {
      "description": "Defines roles for supported projects/service families.",
      "properties": {
        "klat": {
          "allOf": [
            {
              "$ref": "#/$defs/AccessRoles"
            }
          ],
          "default": 0
        },
        "core": {
          "allOf": [
            {
              "$ref": "#/$defs/AccessRoles"
            }
          ],
          "default": 0
        },
        "diana": {
          "allOf": [
            {
              "$ref": "#/$defs/AccessRoles"
            }
          ],
          "default": 0
        },
        "node": {
          "allOf": [
            {
              "$ref": "#/$defs/AccessRoles"
            }
          ],
          "default": 0
        },
        "hub": {
          "allOf": [
            {
              "$ref": "#/$defs/AccessRoles"
            }
          ],
          "default": 0
        },
        "llm": {
          "allOf": [
            {
              "$ref": "#/$defs/AccessRoles"
            }
          ],
          "default": 0
        }
      },
      "title": "PermissionsConfig",
      "type": "object"
    },
    "TokenConfig": {
      "properties": {
        "username": {
          "title": "Username",
          "type": "string"
        },
        "client_id": {
          "title": "Client Id",
          "type": "string"
        },
        "permissions": {
          "additionalProperties": {
            "type": "boolean"
          },
          "title": "Permissions",
          "type": "object"
        },
        "refresh_token": {
          "title": "Refresh Token",
          "type": "string"
        },
        "expiration": {
          "title": "Expiration",
          "type": "integer"
        },
        "refresh_expiration": {
          "title": "Refresh Expiration",
          "type": "integer"
        },
        "token_name": {
          "title": "Token Name",
          "type": "string"
        },
        "creation_timestamp": {
          "title": "Creation Timestamp",
          "type": "integer"
        },
        "last_refresh_timestamp": {
          "title": "Last Refresh Timestamp",
          "type": "integer"
        },
        "access_token": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Access Token"
        }
      },
      "required": [
        "username",
        "client_id",
        "permissions",
        "refresh_token",
        "expiration",
        "refresh_expiration",
        "token_name",
        "creation_timestamp",
        "last_refresh_timestamp"
      ],
      "title": "TokenConfig",
      "type": "object"
    },
    "User": {
      "properties": {
        "username": {
          "title": "Username",
          "type": "string"
        },
        "password_hash": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Password Hash"
        },
        "user_id": {
          "title": "User Id",
          "type": "string"
        },
        "created_timestamp": {
          "title": "Created Timestamp",
          "type": "integer"
        },
        "neon": {
          "allOf": [
            {
              "$ref": "#/$defs/NeonUserConfig"
            }
          ],
          "default": {
            "skills": {},
            "user": {
              "about": "",
              "avatar_url": "",
              "dob": null,
              "email": "",
              "email_verified": false,
              "first_name": "",
              "last_name": "",
              "middle_name": "",
              "phone": "",
              "phone_verified": false,
              "preferred_name": ""
            },
            "language": {
              "input_languages": [
                "en-us"
              ],
              "output_languages": [
                "en-us"
              ]
            },
            "units": {
              "date": "MDY",
              "measure": "imperial",
              "time": 12
            },
            "location": {
              "latitude": null,
              "longitude": null,
              "name": null,
              "timezone": null
            },
            "response_mode": {
              "hesitation": false,
              "limit_dialog": false,
              "tts_gender": "female",
              "tts_speed_multiplier": 1.0
            },
            "privacy": {
              "save_audio": false,
              "save_text": true
            }
          }
        },
        "klat": {
          "allOf": [
            {
              "$ref": "#/$defs/KlatConfig"
            }
          ],
          "default": {
            "is_tmp": true,
            "preferences": {}
          }
        },
        "llm": {
          "allOf": [
            {
              "$ref": "#/$defs/BrainForgeConfig"
            }
          ],
          "default": {
            "inference_access": {}
          }
        },
        "permissions": {
          "allOf": [
            {
              "$ref": "#/$defs/PermissionsConfig"
            }
          ],
          "default": {
            "klat": 0,
            "core": 0,
            "diana": 0,
            "node": 0,
            "hub": 0,
            "llm": 0
          }
        },
        "tokens": {
          "anyOf": [
            {
              "items": {
                "$ref": "#/$defs/TokenConfig"
              },
              "type": "array"
            },
            {
              "type": "null"
            }
          ],
          "default": [],
          "title": "Tokens"
        }
      },
      "required": [
        "username"
      ],
      "title": "User",
      "type": "object"
    },
    "_LanguageConfig": {
      "properties": {
        "input_languages": {
          "default": [
            "en-us"
          ],
          "items": {
            "type": "string"
          },
          "title": "Input Languages",
          "type": "array"
        },
        "output_languages": {
          "default": [
            "en-us"
          ],
          "items": {
            "type": "string"
          },
          "title": "Output Languages",
          "type": "array"
        }
      },
      "title": "_LanguageConfig",
      "type": "object"
    },
    "_LocationConfig": {
      "properties": {
        "latitude": {
          "anyOf": [
            {
              "type": "number"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Latitude"
        },
        "longitude": {
          "anyOf": [
            {
              "type": "number"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Longitude"
        },
        "name": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Name"
        },
        "timezone": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Timezone"
        }
      },
      "title": "_LocationConfig",
      "type": "object"
    },
    "_PrivacyConfig": {
      "properties": {
        "save_text": {
          "default": true,
          "title": "Save Text",
          "type": "boolean"
        },
        "save_audio": {
          "default": false,
          "title": "Save Audio",
          "type": "boolean"
        }
      },
      "title": "_PrivacyConfig",
      "type": "object"
    },
    "_ResponseConfig": {
      "properties": {
        "hesitation": {
          "default": false,
          "title": "Hesitation",
          "type": "boolean"
        },
        "limit_dialog": {
          "default": false,
          "title": "Limit Dialog",
          "type": "boolean"
        },
        "tts_gender": {
          "default": "female",
          "enum": [
            "male",
            "female"
          ],
          "title": "Tts Gender",
          "type": "string"
        },
        "tts_speed_multiplier": {
          "default": 1.0,
          "title": "Tts Speed Multiplier",
          "type": "number"
        }
      },
      "title": "_ResponseConfig",
      "type": "object"
    },
    "_UnitsConfig": {
      "properties": {
        "time": {
          "default": 12,
          "enum": [
            12,
            24
          ],
          "title": "Time",
          "type": "integer"
        },
        "date": {
          "default": "MDY",
          "enum": [
            "MDY",
            "YMD",
            "YDM",
            "DMY"
          ],
          "title": "Date",
          "type": "string"
        },
        "measure": {
          "default": "imperial",
          "enum": [
            "imperial",
            "metric"
          ],
          "title": "Measure",
          "type": "string"
        }
      },
      "title": "_UnitsConfig",
      "type": "object"
    },
    "_UserConfig": {
      "properties": {
        "first_name": {
          "default": "",
          "title": "First Name",
          "type": "string"
        },
        "middle_name": {
          "default": "",
          "title": "Middle Name",
          "type": "string"
        },
        "last_name": {
          "default": "",
          "title": "Last Name",
          "type": "string"
        },
        "preferred_name": {
          "default": "",
          "title": "Preferred Name",
          "type": "string"
        },
        "dob": {
          "anyOf": [
            {
              "format": "date",
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Dob"
        },
        "email": {
          "default": "",
          "title": "Email",
          "type": "string"
        },
        "avatar_url": {
          "default": "",
          "title": "Avatar Url",
          "type": "string"
        },
        "about": {
          "default": "",
          "title": "About",
          "type": "string"
        },
        "phone": {
          "default": "",
          "title": "Phone",
          "type": "string"
        },
        "phone_verified": {
          "default": false,
          "title": "Phone Verified",
          "type": "boolean"
        },
        "email_verified": {
          "default": false,
          "title": "Email Verified",
          "type": "boolean"
        }
      },
      "title": "_UserConfig",
      "type": "object"
    }
  },
  "properties": {
    "operation": {
      "enum": [
        "create",
        "read",
        "update",
        "delete"
      ],
      "title": "Operation",
      "type": "string"
    },
    "username": {
      "title": "Username",
      "type": "string"
    },
    "password": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Password"
    },
    "access_token": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Access Token"
    },
    "user": {
      "anyOf": [
        {
          "$ref": "#/$defs/User"
        },
        {
          "type": "null"
        }
      ],
      "default": null
    }
  },
  "required": [
    "operation",
    "username"
  ],
  "title": "MQRequest",
  "type": "object"
}